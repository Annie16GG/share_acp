{
    "version": 2,
    "builds": [
      {
        "src": "server/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server/api/app.js"
      },
      {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
    ]
  }
  