{
    "version": 2,
    "builds": [
      {
        "src": "server/api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/app/(.*)",
        "dest": "/server/api/app.js"
      },
      {
        "src": "/api/embed/(.*)",
        "dest": "/server/api/embed.js"
      },
      {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
    ]
  }
  