{
  "version": 2,
  "builds": [
    {
      "src": "server/api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/embed.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/app/(.*)",
      "dest": "/server/api/app.js"
    },
    {
      "src": "/api/embed/(.*)",
      "dest": "/server/api/embed.js"
    },
    {
      "src": "/(dashboard|images|forms)/(.*)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
